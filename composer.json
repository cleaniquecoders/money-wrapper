{
    "name": "cleaniquecoders/money-wrapper",
    "description": "MoneyPHP Wrapper",
    "keywords": ["money", "php", "wrapper", "package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\MoneyWrapper\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\MoneyWrapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^5.6|^5.7",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\MoneyWrapper\\MoneyWrapperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
